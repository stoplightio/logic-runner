{
  "name": "@stoplight/logic-runner",
  "version": "1.1.1",
  "description": "Utilities to run flow logic blocks, such as variable replacement, assertions, transforms, etc.",
  "repository": {
    "type": "git",
    "url": "git://github.com/stoplightio/logic-runner.git"
  },
  "main": "dist/bundle.js",
  "scripts": {
    "build": "rollup -c ./rollup.config.js && rollup -c ./rollup.standalone.config.js && rollup -c ./rollup.standalone.config.min.js",
    "test": "nyc ava --verbose",
    "test:watch": "ava --verbose --watch",
    "report-coverage": "nyc report --reporter=lcov | codecov",
    "check-coverage": "nyc check-coverage --statements 75 --branches 75 --functions 75 --lines 75 ",
    "prepublish": "npm run build",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint src/**/*.js"
  },
  "author": "marc@stoplight.io",
  "license": "ISC",
  "pre-commit": [],
  "ava": {
    "files": [
      "**/*test.js"
    ],
    "source": [
      "**/*.{js}",
      "!dist/**/*"
    ],
    "concurrency": 5,
    "failFast": true,
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "ignore": "*test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "devDependencies": {
    "ava": "0.16.x",
    "babel-cli": "6.x.x",
    "babel-core": "6.x.x",
    "babel-plugin-transform-runtime": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-es2015-rollup": "1.x.0",
    "babel-preset-stage-2": "6.x.x",
    "babel-register": "6.x.x",
    "babel-runtime": "6.x.x",
    "codecov": "1.x.x",
    "eslint": "3.x.x",
    "eslint-config-airbnb": "12.x.x",
    "eslint-config-stoplight": "1.x.x",
    "eslint-plugin-import": "2.0.x",
    "eslint-plugin-jsx-a11y": "2.2.x",
    "eslint-plugin-react": "6.3.x",
    "lodash": "4.x.x",
    "nyc": "8.x.x",
    "pre-commit": "1.1.x",
    "rollup": "0.36.x",
    "rollup-plugin-babel": "2.6.x",
    "rollup-plugin-commonjs": "5.0.x",
    "rollup-plugin-node-resolve": "2.0.x",
    "rollup-plugin-uglify": "1.0.x",
    "uglify-js": "mishoo/UglifyJS2#harmony"
  },
  "peerDependencies": {
    "lodash": "4.x.x"
  },
  "dependencies": {
    "crypto-js": "3.x.x",
    "json-stringify-safe": "5.0.x",
    "oauth-1.0a": "2.x.x",
    "qs": "6.x.x"
  }
}
